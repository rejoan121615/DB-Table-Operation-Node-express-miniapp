<section class=" alert-wrap ">
    <div class="container mt-5">
        <div
            class="d-inline-block alert d-inline-flex justify-content-between align-items-center px-2 py-1 <%= alert.class %>"
        >
            <h6 class="mb-0"><%= alert.message %></h6>

            <button
                id="cross-btn"
                type="button"
                class="btn btn-sm btn-outline-danger"
            >
                <i class="fa-solid fa-xmark"></i>
            </button>
        </div>
    </div>
</section>
