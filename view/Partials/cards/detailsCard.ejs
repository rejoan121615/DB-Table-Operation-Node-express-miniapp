<div data-card-name="details" class="card">
    <div class="c-head">
        <p>Type: Visys Details</p>
        <div class="action-list">
            <% if (action.edit) { %>
            <a href="details/<%= data.id %>" class="edit action-btn">
                <img src="/icons/write.png" alt="delete icon" />
            </a>
            <% } %> <% if (action.delete) { %>
            <a href="/delete/details/<%= data.id %>" class="delete action-btn">
                <img src="/icons/bin.png" alt="delete icon" />
            </a>
            <% } %>
        </div>
    </div>
    <div class="c-body">
        <div class="c-item">
            <p>Name:</p>
            <p><%= data.visys_name %></p>
        </div>
        <div class="c-item">
            <p>Url:</p>
            <p><%= data.visys_url %></p>
        </div>
        <div class="c-item">
            <p>Port:</p>
            <p><%= data.visys_port %></p>
        </div>
        <div class="c-item">
            <p>user name:</p>
            <p><%= data.visys_user_name %></p>
        </div>
        <div class="c-item">
            <p>password:</p>
            <p><%= data.visys_user_password %></p>
        </div>
        <div class="c-item">
            <p>flag:</p>
            <p><%= data.visys_flag %></p>
        </div>
    </div>
</div>
