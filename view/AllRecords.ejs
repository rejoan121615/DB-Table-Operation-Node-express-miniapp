<%- include('./Partials/header.ejs') %>
<!-- navbar  -->
<%- include('Partials/navbar.ejs') %>
<!-- retrived result data  -->
<section class="mt-5">
    <h3 class="text-center my-5">All Records List</h3>
    <% if (result.length) { %>
    <div class="container">
        <div
            class="row row-cols-1 row-cols-sm-2 row-cols-lg-3 row-cols-xl-4 mb-3"
        >
            <% result.forEach((data) => { %>
            <%- include('./Partials/details.ejs', {data: data}) %> 
            <% }) %>
        </div>
    </div>
    <% } else { %>
    <h6 class="text-center">No data avaialble please insert a data</h6>
    <% } %>
</section>
<!-- page footer  -->
<%- include('./Partials/footer.ejs') %>
